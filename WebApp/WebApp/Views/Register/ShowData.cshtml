@model List<WebApp.Models.LoginPage>
<h1>List Of Users</h1>

<div class=" container">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Username</th>
                <th>Password</th>
                <th>Action</th>

            </tr>
        </thead>    
        <tbody>
            @for (int i = 0; i < Model.Count(); i++)
            {
                <tr>
                    <td>@Model[i].Username</td>
                    <td>@Model[i].Password</td>
                    <td>
                        <a href="@Url.Action("DeleteData",new {id=Model[i].Id })"><i class="glyphicon glyphicon-trash"></i></a>
                        <a href="@Url.Action("EditData",new {id=Model[i].Id })"><i class="glyphicon glyphicon-edit"></i></a>
                    </td>
                </tr>
            }
        </tbody>
    </table>    

</div>
